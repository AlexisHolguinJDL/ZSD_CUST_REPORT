                                    </m:Column>
                                    <m:Column width="auto" minScreenWidth="Phone">
                                        <m:header>
                                            <m:Text text="{i18n>ProjectManager}" width="auto"/>
                                        </m:header>
                                    </m:Column>
                                </m:columns>
                                <m:items>
                                    <m:ColumnListItem type="Active">
                                        <m:cells>
                                            <m:ObjectIdentifier title="{text}" text="{project_id}"/>
                                            <m:ObjectStatus text="{SoldToPartyName}"/>
                                            <m:ObjectStatus text="{tecn_area}"/>
                                            <m:ObjectStatus text="{geo_area}"/>
                                            <m:ObjectStatus text="{status}"/>
                                            <m:ObjectStatus text="{business_line}"/>
                                            <m:ObjectStatus text="{stype}"/>
                                            <m:ObjectStatus text="{ptype}"/>
                                            <m:Link text="{ManagerName}" href="{ManagerURL}" target="_blank"/>
                                        </m:cells>
                                    </m:ColumnListItem>
                                </m:items>
                            </m:Table>
                        </uxap:blocks>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
        </uxap:sections>
    </uxap:ObjectPageLayout>
</mvc:View>
